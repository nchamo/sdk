  "main": "./dist/index.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsconfig-replace-paths -s src -p tsconfig.json",
    "lint:check": "prettier --check .",
    "lint:fix": "sort-package-json && prettier --write .",
    "prepare": "husky install",
    "script": "ts-node -r tsconfig-paths/register",
    "test": "jest --forceExit --detectOpenHandles --verbose",
    "test:integration": "jest --forceExit --detectOpenHandles --verbose --testPathPattern=test/integration",
    "test:unit": "jest --forceExit --detectOpenHandles --verbose --testPathPattern=test/unit"
  },
  "lint-staged": {
    "*.{js,css,md,ts,sol}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "cross-fetch": "4.1.0",
    "crypto-js": "4.2.0",
    "deepmerge": "4.3.1",
    "lru-cache": "11.1.0",
    "ms": "3.0.0-canary.1",
    "qs": "6.14.0",
    "viem": "2.33.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/chai": "5.2.2",
    "@types/chai-as-promised": "7.1.8",
    "@types/crypto-js": "4.2.2",
    "@types/jest": "29.5.6",
    "@types/node": "24.1.0",
    "@types/qs": "6.14.0",
    "@types/ws": "8.5.10",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.2",
    "dotenv": "16.3.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "13.2.2",
    "patch-package": "8.0.0",
    "prettier": "2.8.8",
    "sort-package-json": "3.4.0",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tsconfig-replace-paths": "0.0.14",
    "typescript": "5.4.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
